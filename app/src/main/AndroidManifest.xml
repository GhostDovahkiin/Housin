<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.housin">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:usesCleartextTraffic="true"
        android:allowBackup="true"
        android:icon="@mipmap/laucher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/laucher"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".view.ContainerActivity"></activity>
        <activity android:name=".view.DenunciaActivity" />
        <activity android:name=".view.FavoritosActivity" />
        <activity android:name=".view.feedActivity" />
        <activity android:name=".view.TermosActivity" />
        <activity android:name=".view.CadastroActivity" />
        <activity android:name=".view.LoginActivity" />
        <activity android:name=".view.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>